{
  "name": "@mattilsynet/design",
  "version": "0.0.4",
  "type": "module",
  "main": "./mtds/index.js",
  "types": "./mtds/index.d.ts",
  "files": [
    "mtds"
  ],
  "exports": {
    ".": {
      "import": "./mtds/index.js",
      "types": "./mtds/index.d.ts"
    },
    "./*.svg": "./mtds/*.svg",
    "./icons/*": "./mtds/icons/*",
    "./illustrations/*": "./mtds/illustrations/*",
    "./package.json": "./package.json",
    "./styles.json": "./mtds/styles.json",
    "./styles.css": "./mtds/styles.css"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build && rm -rf mtds/nedlastinger/",
    "lint": "npx @biomejs/biome lint",
    "preview": "vite preview",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview",
    "publish:patch": "npm run build && npm version patch --message 'Release %' && git push --tags && npm publish --access public",
    "publish:minor": "npm run build && npm version minor --message 'Release %' && git push --tags && npm publish --access public"
  },
  "dependencies": {
    "@digdir/designsystemet-css": "^1.0.0-next.35",
    "@digdir/designsystemet-theme": "^1.0.0-next.35"
  },
  "devDependencies": {
    "@navikt/aksel-icons": "^7.4.3",
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.9.0",
    "@u-elements/u-details": "^0.1.0",
    "@whyframe/core": "^0.1.11",
    "@whyframe/vue": "^0.1.7",
    "postcss": "^8.4.47",
    "postcss-nesting": "^13.0.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.3.0",
    "vitepress": "^1.5.0",
    "vue": "^3.5.12"
  }
}
