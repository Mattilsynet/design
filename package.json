{
	"name": "@mattilsynet/design",
	"version": "2.1.6",
	"type": "module",
	"main": "./mtds/index.js",
	"types": "./mtds/index.d.ts",
	"files": [
		"mtds"
	],
	"engines": {
		"npm": ">=10.0.0",
		"node": ">=20.0.0"
	},
	"exports": {
		".": {
			"import": "./mtds/index.js",
			"types": "./mtds/index.d.ts"
		},
		"./react": {
			"import": "./mtds/react.js",
			"types": "./mtds/react.d.ts"
		},
		"./*.svg": "./mtds/*.svg",
		"./*.ico": "./mtds/*.ico",
		"./icons/*": "./mtds/icons/*",
		"./illustrations/*": "./mtds/illustrations/*",
		"./index.iife.js": "./mtds/index.iife.js",
		"./package.json": "./package.json",
		"./styles.css": "./mtds/styles.css",
		"./styles.json": "./mtds/styles.json",
		"./tailwind.css": "./mtds/tailwind.css"
	},
	"scripts": {
		"build": "vite build --mode iife && vite build && rm -rf mtds/docs/ && rm -rf mtds/iife.css",
		"build:tokens": "npx @digdir/designsystemet tokens build",
		"format": "npx @biomejs/biome format",
		"lint": "npx @biomejs/biome lint",
		"publish:clojure": "npm run build && cd ./clojure && ./publish.sh",
		"start": "vite",
		"storybook": "storybook dev -p 6006",
		"storybook:build": "npm run build && storybook build"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.0",
		"@digdir/designsystemet": "^1.1.10",
		"@digdir/designsystemet-css": "^1.1.10",
		"@phosphor-icons/core": "^2.1.1",
		"@phosphor-icons/react": "^2.1.10",
		"@storybook/addon-docs": "^9.1.2",
		"@storybook/addon-themes": "^9.1.2",
		"@storybook/react-vite": "^9.1.2",
		"@tanstack/react-table": "^8.21.3",
		"@types/node": "^24.3.0",
		"@types/react": "^19.1.10",
		"@types/react-dom": "^19.1.7",
		"@vitejs/plugin-react": "^5.0.1",
		"fast-glob": "^3.3.3",
		"postcss": "^8.5.6",
		"postcss-nesting": "^13.0.2",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"storybook": "^9.1.2",
		"typescript": "^5.9.2",
		"vite": "^7.1.3",
		"vite-plugin-dts": "^4.5.4"
	},
	"dependencies": {
		"@floating-ui/dom": "^1.7.3",
		"@u-elements/u-combobox": "^1.0.0",
		"@u-elements/u-datalist": "^1.0.13",
		"@u-elements/u-details": "^0.1.2",
		"@u-elements/u-progress": "^0.0.5",
		"@u-elements/u-tabs": "^0.0.9",
		"clsx": "^2.1.1"
	},
	"overrides": {
		"tmp": "0.2.5"
	}
}
